{   
	"tokenColors": [
		{
			"settings": {
				"foreground": "#F8F8F2"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#AFBAD4"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#1ABC9C"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#FD9720"
			}
		},
		{
			"name": "Built-in constant",
			"scope": "constant.language",
			"settings": {
				"foreground": "#AE81EF",
                "fontStyle": "italic bold"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#AE81EF",
                "fontStyle": "italic bold"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"fontStyle": "",
                "foreground": "#DDD0F4"
			}
		},
		{
			"name": "Keyword",
			"scope": [
                "keyword.control"
            ],
			"settings": {
				"foreground": "#FF6347",
                "fontStyle": "italic"
			}
		},
        {
            "name": "Break",
            "scope": [
                "keyword.control.c",
                "meta.body.switch.c",
                "meta.block.switch.c",
                "meta.block.c",
                "source.c"
            ],
            "settings": {
                "foreground": "#FF7F50",
                "fontStyle": "italic bold"
            }
        },
        {
			"name": "Logical Operators",
			"scope": [
                "keyword.operator.logical"
            ],
			"settings": {
				"foreground": "#FF6347",
                "fontStyle": "bold"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#BE9AF7"
			}
		},
		{
			"name": "Storage type",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7FDBCA"
			}
		},
		{
			"name": "Class name",
			"scope": "entity.name.class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#54CED6"
			}
		},
		{
			"name": "Function argument",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ECC48D"
			}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#F92672"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"fontStyle": "",
				"foreground": "#A6E22E"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#FF6347"
			}
		},
		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#1ABC9C"                
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#FF6347",
                "fontStyle": "bold underline"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"background": "#F92672",
				"fontStyle": "",
				"foreground": "#F8F8F0"
			}
		},
		{
			"name": "Invalid deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F8F8F0"
			}
		},
        {
            "name": "Struct",
            "scope": "storage.type.struct",
            "settings": {
                "foreground": "#BE9AF7",
                "fontStyle": "italic bold",
            }
        },
        {
            "name": "Macro",
            "scope": "entity.name.function.preprocessor.c",
            "settings": {
                "foreground": "#ECECEC",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Case",
            "scope": "keyword.control.case.c",
            "settings": {
                "foreground": "#EE82EE",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Switch",
            "scope": "keyword.control.switch.c",
            "settings": {
                "foreground": "#EE82EE",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "Sizeof",
            "scope": [
                "keyword.operator.sizeof.c"
            ],
            "settings": {
                "foreground": "#A1EFE4",
                "fontStyle": "bold"
            }
        }
	],
